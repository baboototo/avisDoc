&#13;
&lt;script type="text/javascript"&gt;&#13;
&#13;
	var condSet		= new DATASET("condSet");&#13;
	var dataSet01 	= new DATASET("dataSet01");&#13;
	var trSet01 	= new TRSET("trSet01");	&#13;
	&#13;
	function initWindow(){&#13;
		initWindowTab();&#13;
	}&#13;
	&#13;
	/*&#13;
	 * Tab 초기화 설정&#13;
	 */&#13;
	function initWindowTab(){&#13;
		// Tab Object, Tab Item Title Array Object&#13;
		initComTab(sheetTab, ["상담", "예약", "계약", "이력", "외상매출금경과이력", "쿠폰발행", "DM이력"]);&#13;
	}&#13;
	&#13;
