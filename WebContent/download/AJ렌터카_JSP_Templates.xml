     */&#13;
    function dataSet01_OnLoadCompleted(rowcount){}&#13;
    &#13;
    /*&#13;
     * trSet OnFail Event&#13;
     *  - &lt;script language="javascript" for="trSet01" event="OnFail()"&gt;&#13;
     */&#13;
    function trSet01_OnFail(){}&#13;
    &#13;
