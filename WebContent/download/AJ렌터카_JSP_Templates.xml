	 * IBMDITab OnChange Event&#13;
	 *  - &lt;script language=JavaScript for=tb_Tab event=OnSelChanged(index)&gt;&#13;
	 */&#13;
	function sheetTab_OnChange(curidx, oldidx){}&#13;
	&#13;
	/*&#13;
     * IBSheet OnClick Event&#13;
     */&#13;
	function sheet_OnClick(Row, Col){&#13;
		if(Row &lt; sheet.HeaderRows())	return;&#13;
		var colid = sheet.ColSaveName(Col);&#13;
	}&#13;
	&#13;
	/*&#13;
     * IBSheet OnDblClick Event&#13;
     */&#13;
	function sheet_OnDblClick(Row, Col){&#13;
		if(Row &lt; sheet.HeaderRows())	return;&#13;
		var colid = sheet.ColSaveName(Col);&#13;
	}&#13;
    &#13;
    /*&#13;
     * IBSheet OnLoadData Event&#13;
     *  - OnRowSearchEnd -&gt; OnLoadData 이벤트 사용&#13;
	function sheet_OnLoadData(data){&#13;
		var jsonData = JSON.parse(data);&#13;
		var rowDatas = jsonData.Data;&#13;
	&#13;
		$$.each(rowDatas, function(){&#13;
			// 조회된 JSON 데이터 수정 및 속성값 설정&#13;
		});&#13;
		&#13;
		return JSON.stringify(jsonData);&#13;
	}&#13;
	*/&#13;
	&#13;
	/*&#13;
	 * IBSheet OnRowSearchEnd Event&#13;
	function sheet_OnSort(){&#13;
		sheet.ReNumberSeq();&#13;
		sheet.SetSelectRow(sheet.HeaderRows());&#13;
	}&#13;
	*/&#13;
